<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="wacode.yuki.designsupportlibrarysample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Design.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".kotCode.NavigationView.ktNavigationViewActivity" />
        <activity android:name=".kotCode.CoordinatorLayout.ktCoordinatorLayoutActivity" />
        <activity android:name=".kotCode.InputTextLayout.ktInputTextActivity" />
        <activity android:name=".kotCode.Snackbar.ktSnackBarActivity" />
        <activity android:name=".kotCode.TabLayout.TabLayoutActivity" />
        <activity android:name=".kotCode.FloatActionButton.ktFABActivity"></activity>
        <activity android:name=".java.Animation.ViewPager.ViewPagerFloatButton_Activity"
            android:screenOrientation="portrait"/>
        <activity android:name=".java.FloatingActionButton.javaFloatButton_Activity"
            android:screenOrientation="portrait"/>
        <activity android:name=".java.TextInputLayout.javaTextInputLayout_Activity"
            android:screenOrientation="portrait"/>
        <activity android:name=".java.SnackBar.javaSnackBar_Activity"
            android:screenOrientation="portrait"/>
        <activity android:name=".java.TabLayout.TabLayout_Activity"
            android:screenOrientation="portrait"/>
        <activity android:name=".java.CoordinatorLayout.javaCoordinator_Activity"
            android:screenOrientation="portrait"/>
    </application>

</manifest>
